  <div class="tab-content hidden" id="old-meal-ordered">
    <% if @user.placed_orders.present? %>
      <% @user.placed_orders.passed.each do|meal| %>
        <div class="col-md-8 col-md-offset-2">
          <%= link_to meal_path(meal) do  %>

            <%= render "shared/meal_card", meal: meal %>

          <% end %>
        </div>
      <% end %>
    <% end %>
  </div>
